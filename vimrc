set number
set relativenumber
set nocompatible
set wrap
set laststatus=2
set encoding=utf-8
set termguicolors
set hlsearch
filetype plugin on
syntax on
set tabstop=4
set clipboard=unnamedplus
set mouse=a
set ignorecase
set smartcase
set incsearch
set showmatch
set cursorline
set lines=41
call plug#begin()
call plug#end()


colorscheme catppuccin_latte
